type Query {
    getAllImages: [Image]
}

type Mutation {
    uploadImage(userId: String!, url: String!, fileName: String!, mimeType: String!, size: Int!): Image
    deleteImage(id: String!): Boolean
}


type Image {
    id: String
    userId: String
    url: String
    fileName: String
    uploadedAt: String
    mimeType: String
}
